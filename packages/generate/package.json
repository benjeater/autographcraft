{
  "name": "@autographcraft/generate",
  "version": "1.1.5",
  "description": "The generate application for AutoGraphCraft",
  "main": "dist/app.js",
  "bin": {
    "autographcraft": "dist/app.js"
  },
  "types": "dist/app",
  "type": "module",
  "files": [
    "README.md",
    "README-*.md",
    "dist/**/*.*"
  ],
  "scripts": {
    "clean": "rm -rf dist/*",
    "compile": "tsc -p tsconfig.json && tsc-alias",
    "test": "jest",
    "testW": "jest --watch",
    "bump": "npm version prerelease --no-git-tag-version",
    "bumpPatch": "npm version patch",
    "prepack": "npm run compile && jest",
    "build": "npm run compile",
    "start": "npm run compile && dotenvx run --quiet -- node dist/app.js",
    "startHot": "npm run compile && dotenvx run --quiet -- nodemon -q dist/app.js",
    "startVerbose": "npm run compile && dotenvx run -- nodemon dist/app.js,",
    "lint": "eslint"
  },
  "author": "Ben Jeater <benjeater+autographcraft@gmail.com>",
  "repository": "https://github.com/benjeater/autographcraft",
  "license": "MIT",
  "dependencies": {
    "@autographcraft/core": "^1.1.3",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/plugin-helpers": "^5.1.0",
    "@graphql-codegen/typescript": "4.1.1",
    "@inquirer/prompts": "^7.2.0",
    "jwt-decode": "^4.0.0",
    "open": "^10.1.0",
    "portfinder": "^1.0.32",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.22.0",
    "@eslint/js": "^8.57.1",
    "@jest/globals": "^29.7.0",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "3.3.3",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3",
    "typescript-eslint": "^7.18.0"
  },
  "keywords": []
}
